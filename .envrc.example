# Autoload commands
PATH_add bin

# Atlas
export ATLAS_TOKEN="<your atlas token>"

# DNSimple
export DNSIMPLE_TOKEN="<your dnsimple token>"
export DNSIMPLE_EMAIL="<your dnsimple email>"

## AWS
export AWS_ACCESS_KEY_ID="<your aws access key id>"
export AWS_SECRET_ACCESS_KEY="<your aws secret access key>"
export AWS_REGION="ap-northeast-1"

 Terraform
export TF_VAR_atlas_token=$ATLAS_TOKEN
export TF_VAR_dnsimple_token=$DNSIMPLE_TOKEN
export TF_VAR_dnsimple_email=$DNSIMPLE_EMAIL
